<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Krilauto</title>

  <!--
    - favicon
  -->
  <link rel="shortcut icon" href="/images/logo/MB.png" type="image/x-icon">

  <!--
    - google font link
  -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap"
    rel="stylesheet">

  <!--
    - custom css link
  -->
  <link rel="stylesheet" href="/css/style-prefix.css">
  <link rel="stylesheet" href="/css/table.css">
  <link rel="stylesheet" href="/css/checkout-steps.css">
  <link rel="stylesheet" href="/css/paypal.css" />

  <!-- <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css"> -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css">

  <style>
    .daterangepicker .calendar-table th,
    .daterangepicker .calendar-table td {
      line-height: inherit;
      white-space: inherit;
      vertical-align: inherit;
      min-width: inherit;
      width: inherit;
      height: inherit;
      line-height: inherit;
      border-radius: inherit;
      border: inherit;
      white-space: inherit;
      cursor: pointer;
      /* font-size: inherit; */
      /* text-align: inherit; */
    }

    .daterangepicker td.available,
    .daterangepicker th.available {
      background-color: var(--white)
    }

    .daterangepicker td.in-range {
      background-color: #f8ebeb;
    }

    .daterangepicker td.active,
    .daterangepicker td.active:hover {
      background-color: #1a100f;
      color: #fff;
    }

    table td {
      padding: 10px;
    }

    .daterangepicker .drp-buttons .btn {
      padding: inherit;
    }

    .btn {
      width: inherit;
      height: inherit;
      background: #1a100f;
      border: 1px dotted black;
      font-size: 17px;
      color: #fff;
      padding: inherit;
      border-radius: 4px;
      cursor: pointer;
      margin: 0 auto;
      display: inline-block;
    }

    .next.available {
      color: white !important;
    }

    .header-search-container form {
      display: flex;
      align-items: center;
      justify-content: space-around;
    }

    .header-search-container form input {
      height: fit-content;
      margin: 0;
      border-radius: 0 !important;
    }
  </style>

</head>

<body>


  <div class="overlay" data-overlay></div>

  <!--
    - NOTIFICATION TOAST
  -->

  <div class="notification-toast" data-toast>
    <button class="toast-close-btn" data-toast-close>
      <ion-icon name="close-outline"></ion-icon>
    </button>
    <div class="toast-banner">
      <img src="/images/cars/car.jpeg" alt="The Best Car" width="80" height="70">
    </div>
    <div class="toast-detail">
      <p class="toast-message">
        Someone is new just rented
      </p>
      <p class="toast-title">
        The Best Car
      </p>
      <p class="toast-meta">
        <time datetime="PT2M">2 Minutes</time> ago
      </p>
    </div>
  </div>

  <!--
    - HEADER
  -->

  <header>

    <div class="header-top">

      <div class="container">

        <ul class="header-social-container">

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-facebook"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-twitter"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-instagram"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-linkedin"></ion-icon>
            </a>
          </li>

        </ul>

        <div class="header-alert-news">
          <p>
            <b></b>
          </p>
        </div>

        <div class="header-top-actions">

          <select name="currency">

            <option value="usd">USD &dollar;</option>

          </select>

          <select name="language">
            <option value="fr">Fran&ccedil;ais</option>
          </select>

        </div>

      </div>

    </div>

    <div class="header-main">

      <div class="container">

        <a href="/" class="header-logo">
          <span style="font-weight: bold; color: var(--eerie-black); font-size: 1.5em;">Krilauto</span>
        </a>

        <div class="header-search-container">

          <form action="/search" method="POST" id="search_form">
            <% if(typeof car_name!=='undefined' ) { %>
              <input type="text" name="car_name" class="search-field d-block my-4 mx-auto" placeholder="Car Name..."
                value="<%= car_name ? car_name : ''%>">

              <input data-start="<%= start_date ? start_date : ''%>" data-end="<%= end_date ? end_date : ''%>"
                type="search" name="dates" class="search-field d-block my-4 mx-auto">

              <input type="text" name="city_name" class="search-field d-block my-4 mx-auto" placeholder="City Name..."
                value="<%= city_name ? city_name : ''%>">
              <% } else { %>
                <input type="text" name="car_name" class="search-field d-block my-4 mx-auto" placeholder="Car Name...">

                <input data-start="10/11/2021" data-end="15/11/2022" type="search" name="dates"
                  class="search-field d-block my-4 mx-auto">

                <input type="text" name="city_name" class="search-field d-block my-4 mx-auto"
                  placeholder="City Name...">
                <% } %>

                  <button class="search-btn" type="submit">
                    <ion-icon name="search-outline"></ion-icon>
                  </button>
          </form>

        </div>

        <div class="header-user-actions">

          <a href="/profile" class="action-btn">
            <ion-icon name="person-circle-outline"></ion-icon>
          </a>

          <a href="#" class="action-btn">
            <ion-icon name="notifications-circle-outline"></ion-icon>
            <span class="count">5</span>
          </a>
          <a href="/reserve" class="action-btn">
            <ion-icon name="car-sport-outline"></ion-icon>
            <span class="count-b">
              <ion-icon name="alert-outline"></ion-icon>
            </span>
          </a>

        </div>

      </div>

    </div>

    <nav class="desktop-navigation-menu">

      <div class="container">

        <ul class="desktop-menu-category-list">

          <li class="menu-category">
            <a href="/" class="menu-title">Home</a>
          </li>

          <li class="menu-category">
            <a href="#" class="menu-title">Hot Offers</a>
          </li>

          <li class="menu-category">
            <a href="#" class="menu-title">Special</a>

            <ul class="dropdown-list">

              <li class="dropdown-item">
                <a href="#">New Arrivals</a>
              </li>

              <li class="dropdown-item">
                <a href="#">Trending</a>
              </li>

              <li class="dropdown-item">
                <a href="#">Top Rated</a>
              </li>
            </ul>
          </li>

          <li class="menu-category">
            <a href="/news" class="menu-title">News</a>
          </li>

        </ul>

      </div>

    </nav>

    <div class="mobile-bottom-navigation">

      <button class="action-btn" data-mobile-menu-open-btn>
        <ion-icon name="menu-outline"></ion-icon>
      </button>

      <button class="action-btn">
        <ion-icon name="car-sport-outline" style="display: inline-block"></ion-icon>

        <span class="count-b">
          <ion-icon name="alert-outline" style=" position: absolute;
          top: 5px;
          right: 0px;
          background: var(--bittersweet);
          color: var(--white);
          font-size: 12px;
          font-weight: var(--weight-700);
          line-height: 1;
          padding: 2px 2px;
          -webkit-border-radius: 20px;
          border-radius: 20px;"></ion-icon>
        </span>
      </button>

      <button class="action-btn">
        <ion-icon name="home-outline"></ion-icon>
      </button>

      <button class="action-btn" data-mobile-menu-open-btn>
        <ion-icon name="grid-outline"></ion-icon>
      </button>

    </div>

    <nav class="mobile-navigation-menu  has-scrollbar" data-mobile-menu>

      <div class="menu-top">
        <h2 class="menu-title">Menu</h2>

        <button class="menu-close-btn" data-mobile-menu-close-btn>
          <ion-icon name="close-outline"></ion-icon>
        </button>
      </div>

      <ul class="mobile-menu-category-list">

        <li class="menu-category">
          <a href="/" class="menu-title">Home</a>
        </li>

        <li class="menu-category">
          <button class="accordion-menu" data-accordion-btn>
            <p class="menu-title">Car</p>

            <div>
              <ion-icon name="add-outline" class="add-icon"></ion-icon>
              <ion-icon name="remove-outline" class="remove-icon"></ion-icon>
            </div>
          </button>

          <ul class="submenu-category-list" data-accordion>
            <li class="submenu-category">
              <a href="/car" class="submenu-title">car</a>
            </li>
            <li class="submenu-category">
              <a href="/car" class="submenu-title">car</a>
            </li>
            <li class="submenu-category">
              <a href="/car" class="submenu-title">car</a>
            </li>
            <li class="submenu-category">
              <a href="/car" class="submenu-title">car</a>
            </li>
          </ul>
        </li>

        <li class="menu-category">
          <a href="/" class="menu-title">Home</a>
        </li>

        <li class="menu-category">
          <a href="/" class="menu-title">Home</a>
        </li>

        <li class="menu-category">
          <a href="/" class="menu-title">Home</a>
        </li>

      </ul>

      <div class="menu-bottom">

        <ul class="menu-category-list">

          <li class="menu-category">

            <button class="accordion-menu" data-accordion-btn>
              <p class="menu-title">Language</p>

              <ion-icon name="caret-back-outline" class="caret-back"></ion-icon>
            </button>

            <ul class="submenu-category-list" data-accordion>

              <li class="submenu-category">
                <a href="/" class="submenu-title">English</a>
              </li>

            </ul>

          </li>

          <li class="menu-category">
            <button class="accordion-menu" data-accordion-btn>
              <p class="menu-title">Currency</p>
              <ion-icon name="caret-back-outline" class="caret-back"></ion-icon>
            </button>

            <ul class="submenu-category-list" data-accordion>
              <li class="submenu-category">
                <a href="/" class="submenu-title">USD &dollar;</a>
              </li>
            </ul>
          </li>

        </ul>

        <ul class="menu-social-container">

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-facebook"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-twitter"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-instagram"></ion-icon>
            </a>
          </li>

          <li>
            <a href="#" class="social-link">
              <ion-icon name="logo-linkedin"></ion-icon>
            </a>
          </li>

        </ul>

      </div>

    </nav>

  </header>